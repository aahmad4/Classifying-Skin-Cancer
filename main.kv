<CreateAccountWindow>:
    name: "create"

    namee: namee
    email: email
    password: passw

    FloatLayout:
        cols:1

        FloatLayout:
            size: root.width, root.height/2

            canvas:
                Color:
                    rgb: 54/255, 57/255, 66/255, 1
                Rectangle:
                    pos: self.pos
                    size: self.size

            Label:
                text: "Create an Account"
                size_hint: 0.8, 0.2
                pos_hint: {"x":0.1, "top":1}
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8}
                text: "Name: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8}
                size_hint: 0.4, 0.12
                id: namee
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8-0.13}
                text: "Email: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8-0.13}
                size_hint: 0.4, 0.12
                id: email
                multiline: False
                font_size: (root.width**2 + root.height**2) / 14**4

            Label:
                size_hint: 0.5,0.12
                pos_hint: {"x":0, "top":0.8-0.13*2}
                text: "Password: "
                font_size: (root.width**2 + root.height**2) / 14**4

            TextInput:
                pos_hint: {"x":0.5, "top":0.8-0.13*2}
                size_hint: 0.4, 0.12
                id: passw
                multiline: False
                password: True
                font_size: (root.width**2 + root.height**2) / 14**4

        LabelButton:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.1
            font_size: (root.width**2 + root.height**2) / 15**4
            text: "[u]Already have an account? Log In[/u]"
            markup: True
            on_release:
                root.login()
                root.manager.transition.direction = "left"
                

        Button:
            pos_hint:{"x":0.3,"y":0.25}
            size_hint: 0.4, 0.12
            text: "Submit"
            font_size: (root.width**2 + root.height**2) / 14**4
            on_release:
                root.submit()
                root.manager.transition.direction = "left"
                


<LoginWindow>:
    name: "login"

    email: email
    password: password

    FloatLayout:

        canvas:
            Color:
                rgb: 54/255, 57/255, 66/255, 1
            Rectangle:
                pos: self.pos
                size: self.size
 
        Image:
            source: "images/app_icon.png"
            size_hint: .4, .4
            pos_hint: {"x": .3, "top": 1.06}

        Label:
            text:"Email: "
            font_size: (root.width**2 + root.height**2) / 13**4
            pos_hint: {"x":0.1, "top":0.7}
            size_hint: 0.35, 0.12

        TextInput:
            id: email
            font_size: (root.width**2 + root.height**2) / 13**4
            multiline: False
            pos_hint: {"x": 0.45 , "top":0.7}
            size_hint: 0.4, 0.12

        Label:
            text:"Password: "
            font_size: (root.width**2 + root.height**2) / 13**4
            pos_hint: {"x":0.1, "top":0.55}
            size_hint: 0.35, 0.12

        TextInput:
            id: password
            font_size: (root.width**2 + root.height**2) / 13**4
            multiline: False
            password: True
            pos_hint: {"x": 0.45, "top":0.55}
            size_hint: 0.4, 0.12

        Button:
            pos_hint:{"x":0.3,"y":0.25}
            size_hint: 0.4, 0.12
            font_size: (root.width**2 + root.height**2) / 14**4
            text: "Login"
            on_release:
                root.manager.transition.direction = "up"
                root.loginBtn()

        LabelButton:
            pos_hint:{"x":0.2,"y":0.05}
            size_hint: 0.6, 0.1
            font_size: (root.width**2 + root.height**2) / 15**4
            text: "[u]Don't have an account? Create One[/u]"
            markup: True
            on_release:
                root.manager.transition.direction = "right"
                root.createBtn()


<P>:
    TextInput:
        font_size: 22
        size_hint: .97, 0.9
        pos_hint: {"x": 0.012, "y":0.05}
        multiline: True
        disabled: True
        text: "Dermatol is a skin cancer prevention app which classifies skin lesions, moles, and diseases. We serve the North Carolina community by providing access to local support groups, doctors, and treatment centers. Users can navigate through the app using the app icons on the bottom bar or by using the main images on the app layout. Thank you for using our app!"

<akiec>:
    TextInput:
        font_size: 18
        size_hint: 0.9, 0.6
        pos_hint: {"x": 0.05, "y":.32}
        multiline: True
        disabled: True
        text: "Our machine learning model suggests that this skin shows signs of Actinic keratoses and intrapithelial carcinoma / Bowen's disease (akiec). Keep in mind that the diagnosis is not absolute, and you should check with a doctor to verify. For more information about this disease, please click the link below. Thank you."
    Button:
        font_size: 22
        text: "Continue"
        size_hint: .8, .2
        pos_hint: {"x": .1, "y": .05}
        on_press: root.openLink()

